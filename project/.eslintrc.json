{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "commonjs": true,
        "jquery": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "class-methods-use-this": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off"
    }
}
