{
  "name": "project",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "clientDev": "cd ../frontend && npm run dev",
    "client": "cd ../frontend && npm start",
    "dev": "concurrently -n 'server,client' -c 'blue,green' \"npm run server\" \"npm run clientDev\"",
    "up": "concurrently -n 'server,client' -c 'blue,green' \"npm start\" \"npm run client\"",
    "test": "jest --testTimeout=61000"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-json-validator-middleware": "^2.2.0",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12"
  }
}
